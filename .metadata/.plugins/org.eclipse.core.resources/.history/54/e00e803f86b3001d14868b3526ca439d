package javabasics2;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//OverLoaded methods
		System.out.println(add(1,2,3,4));
		
		
	}
	static int add(int x,int y) {
		return x+y;
	}
	static int add(int x,int y,int z) {
		return x+y+z;
	}
	static int add(int x,int y,int z,int a) {
		return x+y+z+a;
	}

}
